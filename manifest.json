{
    "manifest_version": 3,
    "name": "X",
    "version": "1.0",
    "description": "x",
    "permissions": ["storage", "declarativeContent", "activeTab", "scripting","tabs"],
    "host_permissions": ["https://chat.openai.com/*"],
    "action": {
      "default_popup": "popup.html"
    },
    
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  